package com.kh;

public class Array2 {

	public static void main(String[] args) {
		
		int[] score = new int[3];
		System.out.println(score);
		System.out.println(score.length);
		System.out.println(score[0]);
		System.out.println(score[1]);
		System.out.println(score[2]);
				
		int[][] array = new int[2][3];
		
		
	}

}
