package com.kh;

public class MvpOfToday {

	public static void main(String[] args) {
		
		String[][] names = {
				{"    ", "김재엽",  "    ", "박성모",  "    ", "박현근", "    ", "이규민"},
				{"배지희", "    ",  "유기상", "    ",  "최재훈", "    ", "권영경", "    "},
				{"이준혁", "김강현",  "이한봄", "김무년", "전은우", "김세영", "정윤혜", "김소라"},
				{"김영빈", "    ",  "하태우", "    ",  "조세령", "    ", "최수빈", "    "},
		};

		for(int i=0; i<names.length; i++) {
			for(int j=0; j<names[i].length; j++) {
				System.out.print(names[i][j]);
			}
			System.out.println();
		}
	
	}
}
