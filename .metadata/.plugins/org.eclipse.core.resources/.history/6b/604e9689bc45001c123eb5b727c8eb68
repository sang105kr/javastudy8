package com.kh;

public class Scores {

	public static void main(String[] args) {
		
		boolean stop = false;

		do {
		
			printLine(80);
			System.out.println("1.학생수 | 2.점수입력 | 3.점수리스트 | 4.분석 | 5.종료");
			printLine(80);
			
		}while(!stop);
		
	}
	
	//라인 출력
	public static void printLine(int count) {
		for(int i=1; i<=count ;i++) {
			System.out.print("-");
		}
	}

}
