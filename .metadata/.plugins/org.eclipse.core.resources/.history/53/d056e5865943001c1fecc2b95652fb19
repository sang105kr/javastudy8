package com.kh;

public class Lotto {

	public static void main(String[] args) {
		
		int[] lotto = new int[6];
		boolean stop = false;		
		
		while(!stop) {	
			int num = (int)(Math.random() * 45) + 1;
			
			//중복체크로직!
			//lotto[i] = num;	
		}
		
		for(int i=0; i<lotto.length; i++) {
			System.out.printf(lotto[i] + " ");
		}	
	}

}
