package com.kh;

public class Array1 {

	public static void main(String[] args) {
		
		//배열 생성 : new 사용
		int[] score = new int[3];
		score[0] = 10;
		score[1] = 20;
		score[2] = 30;
		
		for(int i=0; i<score.length; i++) {
			System.out.println(score[i]);
		}
		
		//배열 생성 : {}
		int[] score2 = { 10, 20, 30 };
		
		for(int i=0; i<score2.length; i++) {
			System.out.println(score2[i]);
		}
	}
}
